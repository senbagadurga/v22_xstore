<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="CONSOLE" target="SYSTEM_OUT">
      <PatternLayout pattern="%msg%n" />
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="dtv" level="info" />
    <Root level="warn">
      <AppenderRef ref="CONSOLE" />
    </Root>
  </Loggers>
</Configuration>